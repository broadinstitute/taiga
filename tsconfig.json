{
    "compilerOptions": {
        "outDir": "./taiga2/static/",
        "sourceMap": true,
        "noImplicitAny": false,
        "removeComments": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    },
    "include": [
        "frontend/**/*"
    ],
    "exclude": [
        "node_modules",
        "frontend/node_modules",
        "frontend/src/components/modals/TreeView.tsx"
    ]
}
